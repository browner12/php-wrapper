{
    "name": "fusioncharts/php-wrapper",
    "description": "The FusionCharts PHP server-side wrapper lets you create charts in your PHP website without writing any JavaScript code.",
    "keywords": [
        "fusioncharts",
        "php-wrapper"
    ],
    "homepage": "https://github.com/fusioncharts/php-wrapper",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "FusionCharts\\PhpWrapper\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
